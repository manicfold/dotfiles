! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.font:                           Fira Mono 12
rofi.color-enabled:                  true
!                                        bg,    fg,alt.bg, hi.bg, hi.fg
rofi.color-normal:                   base00,base05,base00,base02,base05
rofi.color-urgent:                   base00,base0B,base00,base02,base0B
rofi.color-active:                   base00,base0C,base00,base02,base0C
rofi.color-window:                   base00,base05
! Separator style (none, dash, solid)
rofi.separator-style:                solid
