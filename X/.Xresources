! ------------------------------------------------------------------------- Xft
Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
! Xft.rgba: rgb
! Xft.hintstyle: hintslight
Xft.rgba: none
Xft.hintstyle: hintfull
Xft.dpi: 96
Xft.lcdfilter: lcddefault

! ----------------------------------------------------------------------- xterm
XTerm*locale: true
XTerm*faceName: Envy Code R:size=10:antialias=false
XTerm*faceSize: 10

! ----------------------------------------------------------------------- urxvt

!URxvt*font:             xft:Fira Mono:size=10
! URxvt.font:xft:Envy Code R:Regular:pixelsize=13
! URxvt.boldFont:xft:Envy Code R:Regular:pixelsize=13
! URxvt.italicFont:xft:Envy Code R:Italic:pixelsize=13
! URxvt.boldItalicFont:xft:Envy Code R:Italic:pixelsize=13
URxvt*font:             xft:Envy Code R for Powerline:Regular:pixelsize=13
URxvt*boldFont:         xft:Envy Code R for Powerline:Bold:pixelsize=13
URxvt*italicFont:       xft:Envy Code R for Powerline:Italic:pixelsize=13
URxvt*boldItalicfont:   xft:Envy Code R for Powerline:Bold Italic:pixelsize=13
URxvt*scrollBar:        false
URxvt*cursorBlink:      1
URxvt*cursorUnderline:  0
URxvt*urgentOnBell:     true
URxvt*letterSpace:      -1.0
URxvt*internalBorder:   2
URxvt.transparent:      false

